{
  "name": "zord-writer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "test": "vitest --coverage",
    "export": "next export",
    "deploy-static": "npm run build && npm run export",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "full-lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix --max-warnings=0",
    "prepare": "cd .. && husky install",
    "postinstall": "npm run prepare",
    "format": "prettier --write .",
    "upgrade-deps": "npx npm-check-updates -u && yarn"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.6",
    "storybook-addon-next": "^1.7.3",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@styled-icons/material-outlined": "^10.47.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.29",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "babel-loader": "^9.1.2",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.1.1",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  },
  "resolutions": {
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  }
}
